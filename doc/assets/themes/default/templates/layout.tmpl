    <title>[: if .Info.Title :][: .Info.Title :][: else :]Office for National Statistics[:end :]: [: .Title :]</title>

    <style>

        a {
            text-decoration: underline;
        }
        a:not('.btn'):hover {
            color: #194E79;
        }
        .nav-active {
            font-weight: 700;
        }

        @media (min-width: 768px) {
            .primary-nav__item:last-of-type a {
                border-right: 1px solid #323132;
            }

        }

        table td,
        table th {
            padding: 7px 16px 9px 16px;
            vertical-align: baseline;
        }

        .table-enhanced {
            display: table;
            border-collapse: collapse;
            background-color: white;
            border-bottom: 1px solid #E8E8E8;
            border-left: 1px solid #E8E8E8;
            border-right: 1px solid #E8E8E8;
        }

        .table-enhanced th {
            text-align: left;
            position: relative;
            background-color: #E8E8E8;
            font-weight: 700;
        }

        .table-enhanced tr:not(:last-of-type) {
            border-bottom: solid 1px #E8E8E8;
        }

        pre {
            white-space: pre-wrap; /* Since CSS 2.1 */
            white-space: -moz-pre-wrap; /* Mozilla, since 1999 */
            white-space: -pre-wrap; /* Opera 4-6 */
            white-space: -o-pre-wrap; /* Opera 7 */
            word-wrap: break-word; /* Internet Explorer 5.5+ */
            font-family: monospace;
        }

        td pre {
            max-width: 220px;
            width: 220px;
        }

        .standout-block {
            border: solid 1px #E8E8E8;
            padding-bottom: 16px;
            margin-bottom: 16px;
            background-color: white;
        }

        .standout-block-padded {
            padding: 7px 16px 9px 16px;
        }

        .standout-block-scroll {
            overflow-x: auto;
        }

        .standout-block pre {
            max-width: 100%;
            width: 100%;
        }

        .page-intro p {
            font-size: 21px;
            margin-top: 0px;
            margin-bottom: 48px;
            padding: 7px 0 9px 0;
            line-height: 32px;
        }

        /*.page-intro p:empty {*/
            /*display: none;*/
        /*}*/

    </style>

  </head>

  <body class="home_page js viewport-lg">

  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MBCBVQS"
                    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->

    [: template "fragments/header" . :]

    <main id="main" role="main" tabindex="-1">
      [: if .SpecificationList :]
        <div class="page-intro background--astral padding-bottom--4" >
            <div class="wrapper">
                <div class="col-wrap">
                    <div class="col">
                        <h1 class="page-intro__title page-intro__title--home"><span class="page-intro__title page-intro__title--home-big">
                          Welcome to the</span> Office for National Statistics Developer Hub
                        </h1>
                    </div>
                </div>
            </div>
        </div>
      [: else :]
        <div class="page-intro background--gallery [: if or .SpecificationSummary .Method :][: if not (.Method.Description) :]padding-bottom--5[: end :][: end :]">
            <div class="wrapper">
                <div class="col-wrap">
                    <div class="col">
                        [: if .SpecificationSummary :]
                            <h1 class="page-intro__title margin-top--5">[:.Info.Title :] reference</h1>
                        [: else if .Title :]
                            <h1 class="page-intro__title margin-top--5">[: .Title :] [: .TitleSuffix :]</h1>
                        [: end :]
                        [: if .Method.Description :]
                            [: safehtml .Method.Description :]
                        [: end :]
                    </div>
                </div>
            </div>
        </div>
      [: end :]
        <div class="wrapper">
            [: template "fragments/body" . :]
        </div>
    </main>

    [: template "fragments/footer" . :]
  </body>
</html>
